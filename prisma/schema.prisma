generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  user_id  String  @id @default(uuid()) // UUID generation handled by Prisma
  username String  @unique
  email    String  @unique
  password String? //Make password optional for Google and GitHub users

  @@map("users")
}
