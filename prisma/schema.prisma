generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         String  @id @default(uuid())
  githubId   String? @unique
  username   String  @unique
  email      String  @unique
  password   String?
  profilePic String
}
